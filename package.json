{
  "name": "nuxt-notify",
  "version": "1.1.3",
  "description": "Advanced toast notifications for Nuxt 3/4 with Tailwind CSS + Nuxt Icon (actions, avatars, progress, themes, UI overrides).",
  "license": "MIT",
  "author": "Nizam Omer (nizaamomer) - https://www.nizaamomer.com",
  "homepage": "https://nuxt-notify.nizaamomer.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nizaamomer/nuxt-notify.git"
  },
  "bugs": {
    "url": "https://github.com/nizaamomer/nuxt-notify/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/nizaamomer"
  },
  "keywords": [
    "nuxt",
    "nuxt-module",
    "nuxt3",
    "nuxt4",
    "toast",
    "notifications",
    "toastify",
    "tailwindcss",
    "tailwind",
    "nuxt-icon",
    "vue",
    "vue3"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/module.mjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.mts",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./styles": "./dist/runtime/styles.css"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "lint": "eslint .",
    "release": "npm run prepack && npm publish"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0 || ^4.0.0",
    "nuxt": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/tailwindcss": {
      "optional": true
    },
    "@tailwindcss/vite": {
      "optional": true
    }
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint-config": "^1.11.0",
    "@nuxt/kit": "^4.2.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@types/node": "latest",
    "eslint": "^9.39.1",
    "nuxt": "^4.2.1",
    "typescript": "~5.9.3",
    "vue-tsc": "^3.1.7"
  }
}
